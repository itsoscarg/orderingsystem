        <div class="info-container">
          <div class="col-xs-3">
            <button id="calc-prices-button" type="button" class="btn btn-success">
             Calculate Prices
           </button>
         </div>
       </div>
        <form action="/order" method="post">
        <button type="submit">Place your Order</button>
