<% if (user) { %>
<% } %>
<form action="/order" method="post">
<ul>
    <% hhh.forEach((oneProduct, i) => { %>
  <section class="info-container">
        </section>
        <section id="items-list" class="container">
          <div class="item row">
            <div class="item-price col-xs-2">
            <img src=  <%= oneProduct.imageUrl %> >
           </div>
            <div class="item-name col-xs-2">
              <span><%= oneProduct.name %> </span>
            </div>
            <div class="item-price col-xs-2">
              <span>$<span id="productPrice<%= i %>"><%= oneProduct.price %></span> per case of 10 </span>
            </div>
            <div class="item-qty col-xs-3">
              <label>QTY</label>
              <input data-prev="0" data-index=" <%= i %>" class="quantity input" name="qnt<%= i %>" value="0">
            </div>

          </div>
        </section>
          <% }) %>
        </ul>
        <div class="section">
              <div class="container">
                <div class="row">
                  <div class="col-md-12">
                    <div class="row text-center">
                      <div class="col-md-12">
                        <button id="calc-prices-button" type="button" class="btn btn-success">Calculate Prices</button>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="section">
              <div class="container">
                <h1 class="text-right">TOTAL PRICE:
                  <span id="subtotal">$0.00</span>
                </h1>
              </div>
              <div class="section">
      <div class="container">
